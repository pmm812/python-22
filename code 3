from math import factorial as fact

def main():
    try:
        number = int(input().strip())
        if number < 0:
            print("Ошибка: факториал определен только для неотрицательных чисел")
        else:
            result = fact(number)
            print(result)
    except ValueError:
        print("Ошибка: введите целое число")

if __name__ == "__main__":
    main()
